//----------------------------------------------------------
//Important variables
//----------------------------------------------------------

d := 70 // cylinder of diameter
l := 1350 // cylinder height
ACo := 31e-12
MCo := 1440e3
K1Co := 410e3
ANi := 8e-12
MNi := 490e3
K1Ni := -5e3
cl := 2.5

//----------------------------------------------------------
//Universe and cells
//----------------------------------------------------------

SetGridSize(d/cl,d/cl,l/cl)
SetCellSize(2.5e-9, 2.5e-9, 2.5e-9)

//----------------------------------------------------------
//Geometry
//----------------------------------------------------------

a := cylinder(70e-9, 1350e-9)
setgeom( a )
saveas(m, "geom")

//----------------------------------------------------------
//Regions
//----------------------------------------------------------

defregion(1, zrange(-675e-9, -225e-9))  // first cylinder 
defregion(2, zrange(-225e-9,225e-9))   // second cylinder  
defregion(3, zrange(75e-9, 675e-9))   // third cylinder   
save(regions)
//----------------------------------------------------------
//Parameters region 1
//----------------------------------------------------------
  
Msat.setregion(1, MCo) //material parameters of Ni (Soft)
Aex.setregion(1, ACo)
Ku1.setregion(1, K1Co)
anisU.setregion(1, vector(0,0,1))

     
//----------------------------------------------------------
//Parameters region 2
//----------------------------------------------------------

Msat.setregion(2, MCo) //material parameters of Ni (Soft)
Aex.setregion(2, ACo)
Ku1.setregion(2, K1Co)
anisU.setregion(2, vector(0,0,1))

//----------------------------------------------------------
//Parameters region 3
//----------------------------------------------------------

Msat.setregion(3, MCo)    //material parameters of Ni (Soft)
Aex.setregion(3, ACo)
Ku1.setregion(3, K1Co)
anisU.setregion(3, vector(0,0,1))



Alpha = 0.008

m.loadfile("1350_H1.ovf")

TableAdd(m_full)
TableAdd(B_ext)
TableAdd(E_exch)
TableAdd(E_demag)
TableAdd(E_zeeman)
TableAdd(E_total)

Amp := 0.001
f := 50e+9
t0 := 1.0e-9
omega := 2 * pi * f
B_ext = vector(Amp*sin(omega*(t-t0))/(omega*(t-t0)),0,0)
tableautosave(10e-12)
run(100e-9)
